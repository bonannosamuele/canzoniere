{
  "notes": {
    "system": "latin"
  },
  "pdf": {
    "pagealign-songs": 0,
    "even-odd-pages": 0,
    "formats": {
      "title": {
        "subtitle": [
          "%{artist}",
          "",
          ""
        ]
      },
      "first": {
        "footer": [
          "",
          "",
          "%{page}"
        ]
      }
    },
    "fonts": {
      "text": {
        "description": "Times-Roman",
        "size": 10
      },
      "chord": {
        "description": "sans italic",
        "color": "#0000FF",
        "size": 9
      },
      "comment": {
        "description": "sans italic",
        "size": 10,
        "background": "none"
      }
    },
    "chorus": {
      "tag": "Rit.",
      "recall": {
        "tag": "Rit."
      }
    },
    "labels": {
      "width": "auto"
    },
    "diagrams": {
      "show": "bottom",
      "height": 6,
      "width": 6
    }
  },
  "contents": [
    {
      "fields": [
        "sorttitle"
      ],
      "label": "Canzoni in ordine alfabetico",
      "line": "%{title}%{artist| - %{}}",
      "pageno": "%{page}",
      "omit": false,
      "template": "stdtoc"
    },
    {
      "fields": [
        "tag",
        "sorttitle"
      ],
      "label": "Canzoni per tipo",
      "break": "\n%{tag|%{}|Unspecified}",
      "line": "%{title}%{artist| - %{}}",
      "pageno": "%{page}",
      "omit": false
    }
  ]
}
